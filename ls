[32m+[m[32m// En estas primeras 6 preguntas, reemplaza `null` por la respuesta[m
[32m+[m
[32m+[m[32m// Crea una variable "string", puede contener lo que quieras:[m
[32m+[m[32mconst nuevaString = null;[m
[32m+[m
[32m+[m[32m// Crea una variable num√©rica, puede ser cualquier n√∫mero:[m
[32m+[m[32mconst nuevoNum = null;[m
[32m+[m
[32m+[m[32m// Crea una variable booleana:[m
[32m+[m[32mconst nuevoBool = null;[m
[32m+[m
[32m+[m[32m// Resuelve el siguiente problema matem√°tico:[m
[32m+[m[32mconst nuevaResta = 10 - null === 5;[m
[32m+[m
[32m+[m[32m// Resuelve el siguiente problema matem√°tico:[m
[32m+[m[32mconst nuevaMultiplicacion = 10 * null === 40 ;[m
[32m+[m
[32m+[m[32m// Resuelve el siguiente problema matem√°tico:[m
[32m+[m[32mconst nuevoModulo = 21 % 5 === null;[m
[32m+[m
[32m+[m
[32m+[m[32m// En los pr√≥ximos 22 problemas, deber√°s completar la funci√≥n.[m
[32m+[m[32m// Todo tu c√≥digo ir√° dentro de las llaves de la funci√≥n.[m
[32m+[m[32m// Aseg√∫rate que usas "return" cuando la consola te lo pida.[m
[32m+[m[32m// Pista: "console.log()" NO fucionar√°.[m
[32m+[m[32m// No cambies los nombres de las funciones.[m
[32m+[m
[32m+[m[32mfunction devolverString(str) {[m
[32m+[m[32m  // "Return" la string provista: str[m
[32m+[m[32m  // Tu c√≥digo:[m
[32m+[m[41m  [m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction suma(x, y) {[m
[32m+[m[32m  // "x" e "y" son n√∫meros[m
[32m+[m[32m  // Suma "x" e "y" juntos y devuelve el valor[m
[32m+[m[32m  // Tu c√≥digo:[m
[32m+[m[41m  [m
[32m+[m[32m}[m
[32m+[m
