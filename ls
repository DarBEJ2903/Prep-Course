[32m+[m[32m// No cambies los nombres de las funciones.[m
[32m+[m
[32m+[m[32mfunction deObjetoAmatriz(objeto){[m
[32m+[m[32m  // Escribe una funci√≥n que convierta un objeto en una matriz, donde cada elemento representa[m[41m [m
[32m+[m[32m  // un par clave-valor en forma de matriz.[m
[32m+[m[32m  //Ejemplo:[m[41m [m
[32m+[m[32m  /*objeto({[m
[32m+[m[32m      D: 1,[m
[32m+[m[32m      B: 2,[m
[32m+[m[32m      C: 3[m
[32m+[m[32m    }) ‚ûû [["D", 1], ["B", 2], ["C", 3]]*/[m
[32m+[m[32m  //Escribe tu c√≥digo aqu√≠[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction numberOfCharacters(string) {[m
[32m+[m[32m  //La funci√≥n recibe un string. Recorre el srting y devuelve el caracter con el n√∫mero de veces que aparece[m[41m [m
[32m+[m[32m  //en formato par clave-valor.[m
[32m+[m[32m  //Ej: Recibe ---> "adsjfdsfsfjsdjfhacabcsbajda" || Devuelve ---> { a: 5, b: 2, c: 2, d: 4, f: 4, h:1, j: 4, s: 5 }[m[41m [m
[32m+[m[32m  //Escribe tu c√≥digo aqu√≠[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction capToFront(s) {[m
[32m+[m[32m  //Realiza una funci√≥n que reciba como par√°metro un string y mueva todas las letras may√∫sculas[m
[32m+[m[32m  //al principio de la palabra.[m
[32m+[m[32m  //Ejemplo: soyHENRY -> HENRYsoy[m
[32m+[m[32m  //Escribe tu c√≥digo aqu√≠[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction asAmirror(str) {[m
[32m+[m[32m  //La funci√≥n recibe una frase.[m[41m [m
[32m+[m[32m  //Escribe una funci√≥n que tome la frase recibida y la devuelva de modo tal que se pueda leer de izquierda a derecha[m[41m [m
[32m+[m[32m  //pero con cada una de sus palabras invertidas, como si fuera un espejo.[m
[32m+[m[32m  //Ej: Recibe ---> "The Henry Challenge is close!" || Devuelve ---> "ehT yrneH egnellahC si !esolc"[m
[32m+[m[32m  //Escribe tu c√≥digo aqu√≠[m
[32m+[m[32m}[m[41m [m
[32m+[m
[32m+[m
[32m+[m[32mfunction capicua(numero){[m
[32m+[m[32m  //Escribe una funci√≥n, la cual recibe un n√∫mero y determina si es o no capic√∫a.[m
[32m+[m[32m  //La misma debe retornar: "Es capicua" si el n√∫mero se n√∫mero que se lee igual de[m[41m [m
[32m+[m[32m  //izquierda a derecha que de derecha a izquierda. Caso contrario retorna "No es capicua"[m
[32m+[m[32m  //Escribe tu c√≥digo aqu√≠[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction deleteAbc(cadena){[m
[32m+[m[32m  //Define una funci√≥n que elimine las letras "a", "b" y "c" de la cadena dada[m[41m [m
[32m+[m[32m  //y devuelva la versi√≥n modificada o la misma cadena, en caso de contener dichas letras.[m
[32m+[m[32m  //Escribe tu c√≥digo aqu√≠[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction sortArray(arr) {[m
[32m+[m[32m  //La funci√≥n recibe una matriz de strings. Ordena la matriz en orden creciente de longitudes de cadena[m
[32m+[m[32m  //Ej: Recibe ---> ["You", "are", "beautiful", "looking"] || Devuelve ---> [‚ÄúYou", "are", "looking", "beautiful"][m
[32m+[m[32m  //Escribe tu c√≥digo aqu√≠[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction buscoInterseccion(arreglo1, arreglo2){[m
[32m+[m[32m  //Existen dos arrays, cada uno con 5 n√∫meros. A partir de ello, escribir una funci√≥n que permita[m[41m [m
[32m+[m[32m  //retornar un nuevo array con la intersecci√≥n de ambos elementos. (Ej: [4,2,3] uni√≥n [1,3,4] = [3,4].[m
[32m+[m[32m  //Si no tienen elementos en com√∫n, retornar un arreglo vac√≠o.[m
[32m+[m[32m  //Aclaraci√≥n: los arreglos no necesariamente tienen la misma longitud[m
[32m+[m[32m  //Escribe tu c√≥digo aqu√≠[m[41m  [m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m// No modificar nada debajo de esta l√≠nea[m
[32m+[m[32m// --------------------------------[m
[32m+[m
[32m+[m[32mmodule.exports = {[m
[32m+[m[32m   deObjetoAmatriz,[m
[32m+[m[32m   numberOfCharacters,[m
[32m+[m[32m   capToFront,[m
[32m+[m[32m   asAmirror,[m
[32m+[m[32m   capicua,[m
[32m+[m[32m   deleteAbc,[m
[32m+[m[32m   sortArray,[m
[32m+[m[32m   buscoInterseccion,[m
[32m+[m[32m};[m
[32m+[m
[1mdiff --git a/10-JS-Extra-Homeworks/homework/tests/JSX.test.js b/10-JS-Extra-Homeworks/homework/tests/JSX.test.js[m
[1mnew file mode 100644[m
[1mindex 0000000..c0a219e[m
[1m--- /dev/null[m
[1m+++ b/10-JS-Extra-Homeworks/homework/tests/JSX.test.js[m
[36m@@ -0,0 +1,78 @@[m
[32m+[m[32m/* eslint-disable no-undef */[m
[32m+[m[32mconst {[m
[32m+[m[32m    deObjetoAmatriz,[m
[32m+[m[32m    numberOfCharacters,[m
[32m+[m[32m    capToFront,[m
[32m+[m[32m    asAmirror,[m
[32m+[m[32m    capicua,[m
[32m+[m[32m    deleteAbc,[m
[32m+[m[32m    sortArray,[m
[32m+[m[32m    buscoInterseccion,[m
[32m+[m[32m} = require('../homework');[m
[32m+[m
[32m+[m
[32m+[m[32mdescribe('deObjetoAmatriz(objeto)', function() {[m
[32m+[m[32m  it('should return an array, where each element represents a key-value pair in the form of an array.', function() {[m
[32m+[m[32m    expect(deObjetoAmatriz({ x: 1, y: 2 })).toEqual([["x" , 1],["y" , 2]]);[m
[32m+[m[32m    expect(deObjetoAmatriz({ x: 10, y: 25 })).toEqual([["x" , 10],["y" , 25]]);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('numberOfCharacters(string)', function() {[m
[32m+[m[32m  it('should return an object with the characters and the number of times they appear as a key-value pair', function() {[m
[32m+[m[32m    expect(numberOfCharacters("sktpwrroqstkrpwwsqtqopwktsd")).toEqual({s:4, k:3, t:4, p:3, w:4, r:3, o:2, q:3, d:1 });[m
[32m+[m[32m    expect(numberOfCharacters("adsjfdsfsfjsdjfhacabcsbajda")).toEqual({ a: 5, b: 2, c: 2, d: 4, f: 4, h:1, j: 4, s: 5 });[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('capToFront(s)', function() {[m
[32m+[m[32m    it('should return all capital letters at the beginning of the word.', function() {[m
[32m+[m[32m    expect(capToFront("soyHENRY")).toEqual("HENRYsoy");[m
[32m+[m[32m    expect(capToFront("DESArrollaDOR")).toEqual("DESADORrrolla");[m
[32m+[m[32m  });[m
[32m+[m[32m});[m[41m [m
[32m+[m
[32m+[m[32mdescribe('asAmirror(str)', function() {[m
[32m+[m[32m  it('should return the string recieved with all the words reversed as a mirror', function() {[m
[32m+[m[32m    expect(asAmirror("I love you so much!")).toBe("I evol uoy os !hcum");[m
[32m+[m[32m    expect(asAmirror("The Henry Challenge is close!")).toBe("ehT yrneH egnellahC si !esolc");[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('capicua(numero)', function() {[m
[32m+[m[32m  it('should return Es capicua if the number is read from right to left', function() {[m
[32m+[m[32m    expect(capicua(12321)).toEqual("Es capicua");[m
[32m+[m[32m    expect(capicua(1111)).toEqual("Es capicua");[m
[32m+[m[32m    expect(capicua(105217)).toEqual("No es capicua");[m
[32m+[m[32m    expect(capicua(7878700)).toEqual("No es capicua");[m
[32m+[m
[32m+[m[32m  });[m
[32m+[m[32m});[m[41m  [m
[32m+[m
[32m+[m[32mdescribe('deleteAbc(cadena)', function() {[m
[32m+[m[32m  it('should return the modified string if it has the letters abc or the same string otherwise', function() {[m
[32m+[m[32m    expect(deleteAbc("abcefgh")).toEqual("efgh");[m
[32m+[m[32m    expect(deleteAbc("abc")).toEqual("");[m
[32m+[m[32m    expect(deleteAbc("plural")).toEqual("plurl");[m
[32m+[m[32m    expect(deleteAbc("limon")).toEqual("limon");[m
[32m+[m
[32m+[m[32m  });[m
[32m+[m[32m});[m[41m  [m
[32m+[m
[32m+[m[32mdescribe('sortArray(array)', function() {[m
[32m+[m[32m  it('should return a new array in increasing order', function() {[m
[32m+[m[32m    expect(sortArray(["You", "are", "beautiful", "looking"])).toEqual(["You", "are", "looking", "beautiful"]);[m
[32m+[m[32m    expect(sortArray(["pera", "manzana", "alcaucil", "papa"])).toEqual(["pera", "papa", "manzana", "alcaucil"]);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('buscoInterseccion(arreglo1, arreglo2)', function() {[m
[32m+[m[32m  it('should return the common elements of the array, and in case they do not match, return an empty array', function() {[m
[32m+[m[32m    expect(buscoInterseccion([1, 2 , 3], [1, 5, 8 , 3])).toEqual([1, 3]);[m
[32m+[m[32m    expect(buscoInterseccion([7 , 23, 4], [23, 70])).toEqual([23]);[m
[32m+[m[32m    expect(buscoInterseccion([1, 20 , 3], [22, 5, 7])).toEqual([]);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m[41m    [m
[32m+[m
[32m+[m
[32m+[m
