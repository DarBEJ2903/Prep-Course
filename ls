[32m+[m[32mdescribe('crearGato(nombre, edad)', function() {[m
[32m+[m[32m  it('should create a new cat with the nombre and edad properties set', function() {[m
[32m+[m[32m    expect(crearGato('Snowball', 1).nombre).toBe('Snowball');[m
[32m+[m[32m    expect(crearGato('Snowball', 1).edad).toBe(1);[m
[32m+[m[32m    expect(crearGato('Snowball II', 5).nombre).toBe('Snowball II');[m
[32m+[m[32m    expect(crearGato('Snowball II', 5).edad).toBe(5);[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should add a method called meow to the new cat object', function() {[m
[32m+[m[32m    expect(crearGato('Snowball III', 2).meow()).toBe('Meow!');[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('agregarPropiedad(object, property)', function() {[m
[32m+[m[32m  it('should add the property to the object with a value of null', function() {[m
[32m+[m[32m    const object = {[m
[32m+[m[32m      x: 1,[m
[32m+[m[32m      y: 2,[m
[32m+[m[32m    };[m
[32m+[m[32m    const updatedObject = {[m
[32m+[m[32m      x: 1,[m
[32m+[m[32m      y: 2,[m
[32m+[m[32m      z: null,[m
[32m+[m[32m    };[m
[32m+[m[32m    expect(agregarPropiedad(object, 'z')).toEqual(updatedObject);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('invocarMetodo(object, method)', function() {[m
[32m+[m[32m  it('should invoke the method on the object', function() {[m
[32m+[m[32m    const object = {[m
[32m+[m[32m      x: 0,[m
[32m+[m[32m      incrementX: function() {[m
[32m+[m[32m        this.x++;[m
[32m+[m[32m      },[m
[32m+[m[32m    }[m
[32m+[m[32m    invocarMetodo(object, 'incrementX');[m
[32m+[m[32m    expect(object.x).toBe(1);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('multiplicarNumeroDesconocidoPorCinco(mysteryNumberObject)', function() {[m
[32m+[m[32m  it('should return the mysteryNumber property multiplied by five', function() {[m
[32m+[m[32m    const mysteryBox = {[m
[32m+[m[32m      numeroMisterioso: 999,[m
[32m+[m[32m    };[m
[32m+[m[32m    expect(multiplicarNumeroDesconocidoPorCinco(mysteryBox)).toBe(4995);[m
[32m+[m[32m    mysteryBox.numeroMisterioso = -5;[m
[32m+[m[32m    expect(multiplicarNumeroDesconocidoPorCinco(mysteryBox)).toBe(-25);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('eliminarPropiedad(object, property)', function() {[m
[32m+[m[32m  it('should delete the property from the object', function() {[m
[32m+[m[32m    const updatedObject = {[m
[32m+[m[32m      x: 1,[m
[32m+[m[32m      y: 2,[m
[32m+[m[32m    };[m
[32m+[m[32m    const object = {[m
[32m+[m[32m      x: 1,[m
[32m+[m[32m      y: 2,[m
[32m+[m[32m      z: null,[m
[32m+[m[32m    };[m
[32m+[m[32m    expect(eliminarPropiedad(object, 'z')).toEqual(updatedObject);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('nuevoUsuario(nombre, email, password)', function() {[m
[32m+[m[32m  it('should return a new user object with a nombre, email, and password property that match the arguments', function() {[m
[32m+[m[32m    const user = {[m
[32m+[m[32m      nombre: 'Jhon',[m
[32m+[m[32m      email: 'jhon@email.com',[m
[32m+[m[32m      password: 'correcthorsebatterystaple',[m
[32m+[m[32m    };[m
[32m+[m[32m    expect(nuevoUsuario(user.nombre, user.email, user.password)).toEqual(user);[m
[32m+[m[32m    const user2 = {[m
[32m+[m[32m      nombre: 'Samuel',[m
[32m+[m[32m      email: 'samuel@email.com',[m
[32m+[m[32m      password: 'password',[m
[32m+[m[32m    };[m
[32m+[m[32m    expect(nuevoUsuario(user2.nombre, user2.email, user2.password)).toEqual(user2);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('tieneEmail(user)', function() {[m
[32m+[m[32m  it('should return true if the user object has a value for its email property', function() {[m
[32m+[m[32m    expect(tieneEmail({ usernombre: 'Samuel', email: 'samuel@email.com' })).toEqual(true);[m
[32m+[m[32m    expect(tieneEmail({ usernombre: 'Jhon', email: null })).toEqual(false);[m
[32m+[m[32m    expect(tieneEmail({ usernombre: 'Antonio' })).toEqual(false);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('tienePropiedad(object, property)', function() {[m
[32m+[m[32m  it('should return true if the object has the property that is passed in', function() {[m
[32m+[m[32m    const obj = {[m
[32m+[m[32m      x: true,[m
[32m+[m[32m    };[m
[32m+[m[32m    expect(tienePropiedad(obj, 'x')).toEqual(true);[m
[32m+[m[32m    expect(tienePropiedad(obj, 'y')).toEqual(false);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('verificarPassword(user, password)', function() {[m
[32m+[m[32m  it('should return true if passwords match', function() {[m
[32m+[m[32m    const user = {[m
[32m+[m[32m      password: 'Me encanta JS!',[m
[32m+[m[32m    };[m
[32m+[m[32m    const password = 'Me encanta JS!';[m
[32m+[m[32m    expect(verificarPassword(user, password)).toBe(true);[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return false if passwords do not match', function() {[m
[32m+[m[32m    const user = {[m
[32m+[m[32m      password: 'Me encanta JS!',[m
[32m+[m[32m    };[m
[32m+[m[32m    const password = 'Hacker time!';[m
[32m+[m[32m    expect(verificarPassword(user, password)).toBe(false);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('actualizarPassword(user, password)', function() {[m
[32m+[m[32m  it('should return the user object with the updated password', function() {[m
[32m+[m[32m    const user = {[m
[32m+[m[32m      password: 'Me encanta JS!',[m
[32m+[m[32m    };[m
[32m+[m[32m    const password = 'Me encanta JS mucho m√°s!';[m
[32m+[m[32m    expect(actualizarPassword(user, password).password).toBe(password);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('agregarAmigo(user, newFriend)', function() {[m
[32m+[m[32m  it('should add a new friend to the end of the amigos array property', function() {[m
[32m+[m[32m    const user = {[m
[32m+[m[32m      amigos: ['Ben', 'Austen', 'Ryan', 'Mike', 'Young'],[m
[32m+[m[32m    };[m
[32m+[m[32m    const newFriend = 'Shay';[m
[32m+[m[32m    expect(agregarAmigo(user, 'Shay').amigos.pop()).toBe('Shay');[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('pasarUsuarioAPremium(users)', function() {[m
[32m+[m[32m  it('should return the users array with each user\'s esPremium property set to true', function() {[m
[32m+[m[32m    const users = [[m
[32m+[m[32m      { esPremium: false },[m
[32m+[m[32m      { esPremium: false },[m
[32m+[m[32m      { esPremium: false },[m
[32m+[m[32m      { esPremium: false },[m
[32m+[m[32m      { esPremium: false },[m
[32m+[m[32m    ];[m
[32m+[m[32m    const updatedUsers = [[m
[32m+[m[32m      { esPremium: true },[m
[32m+[m[32m      { esPremium: true },[m
[32m+[m[32m      { esPremium: true },[m
[32m+[m[32m      { esPremium: true },[m
[32m+[m[32m      { esPremium: true },[m
[32m+[m[32m    ];[m
[32m+[m[32m    expect(pasarUsuarioAPremium(users)).toEqual(updatedUsers);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('sumarLikesDeUsuario(user)', function() {[m
[32m+[m[32m  it('should return the sum of likes for all user posts', function() {[m
[32m+[m[32m    const user = {[m
[32m+[m[32m      usernombre: 'Jhon Doe',[m
[32m+[m[32m      password: 'JavaScript es genial!',[m
[32m+[m[32m      posts: [[m
[32m+[m[32m        {[m
[32m+[m[32m          id: '1',[m
[32m+[m[32m          title: 'Aventuras en JS!',[m
[32m+[m[32m          likes: 10[m
[32m+[m[32m        },[m
[32m+[m[32m        {[m
[32m+[m[32m          id: '2',[m
[32m+[m[32m          title: 'Soy Henry!',[m
[32m+[m[32m          likes: 100[m
[32m+[m[32m        },[m
[32m+[m[32m        {[m
[32m+[m[32m          id: '3',[m
[32m+[m[32m          title: 'Qu√© es un JavaScript?',[m
[32m+[m[32m          likes: 35[m
[32m+[m[32m        },[m
[32m+[m[32m        {[m
[32m+[m[32m          id: '4',[m
[32m+[m[32m          title: 'JS Objects for Dummies',[m
[32m+[m[32m          likes: 42[m
[32m+[m[32m        },[m
[32m+[m[32m        {[m
[32m+[m[32m          id: '5',[m
[32m+[m[32m          title: 'Educaci√≥n online',[m
[32m+[m[32m          likes: 99[m
[32m+[m[32m        },[m
[32m+[m[32m      ],[m
[32m+[m[32m    };[m
[32m+[m[32m    expect(sumarLikesDeUsuario(user)).toBe(286);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('agregarMetodoCalculoDescuento(storeItem)', function() {[m
[32m+[m[32m  const storeItem = {[m
[32m+[m[32m    precio: 80,[m
[32m+[m[32m    porcentajeDeDescuento: 0.1,[m
[32m+[m[32m  };[m
[32m+[m[32m  const storeItem2 = {[m
[32m+[m[32m    precio: 5,[m
[32m+[m[32m    porcentajeDeDescuento: 0.5,[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  it('should add the method \'calcularPrecioDescuento\' to the store item object', function() {[m
[32m+[m[32m    expect(agregarMetodoCalculoDescuento(storeItem).calcularPrecioDescuento).toBeDefined();[m
[32m+[m[32m    expect(agregarMetodoCalculoDescuento(storeItem2).calcularPrecioDescuento).toBeDefined();[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return the discount precio from the new \'precioConDescuento\' method', function() {[m
[32m+[m[32m    expect(agregarMetodoCalculoDescuento(storeItem).calcularPrecioDescuento()).toBe(72);[m
[32m+[m[32m    expect(agregarMetodoCalculoDescuento(storeItem2).calcularPrecioDescuento()).toBe(2.5);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
