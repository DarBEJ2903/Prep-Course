[32m+[m[32m  html: true,[m
[32m+[m[32m  breaks: true,[m
[32m+[m[32m  linkify: true,[m
[32m+[m[32m  typographer: true,[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mconst mdAnchorOpts = {[m
[32m+[m[32m  permalink: true,[m
[32m+[m[32m  permalinkClass: "anchor-link",[m
[32m+[m[32m  permalinkSymbol: "",[m
[32m+[m[32m  level: [1, 2, 3, 4],[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = function (eleventyConfig) {[m
[32m+[m
[32m+[m[32m  eleventyConfig.setLibrary([m
[32m+[m[32m    "md",[m
[32m+[m[32m    markdownIt(mdOptions)[m
[32m+[m[32m      .use(markdownItAnchor, mdAnchorOpts)[m
[32m+[m[32m      .use(markdownItHighlightJS)[m
[32m+[m[32m  );[m
[32m+[m
[32m+[m[32m  eleventyConfig.addPlugin(eleventyNavigationPlugin);[m
[32m+[m[32m  eleventyConfig.addPlugin(readingTime);[m
[32m+[m[32m  eleventyConfig.addPlugin(readerBar);[m
[32m+[m[32m  eleventyConfig.addPlugin(syntaxHighlight);[m
[32m+[m
[32m+[m[32m  eleventyConfig.addPlugin(pluginTOC, {[m
[32m+[m[32m    tags: ['h2', 'h3'],[m
[32m+[m[32m    ul: true[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m  eleventyConfig.addPassthroughCopy("_src/styles");[m
[32m+[m[32m  eleventyConfig.addPassthroughCopy("_src/assets");[m
[32m+[m[32m  eleventyConfig.addPassthroughCopy("_src/fonts");[m
[32m+[m
[32m+[m[32m  eleventyConfig.addNunjucksFilter('bootstrapNav', toBootstrapNav)[m
[32m+[m
[32m+[m[32m  eleventyConfig.addLinter("Spelling check", function(content, inputPath, outputPath) {[m
[32m+[m[32m    let words = "lenght, .lenght, .rigth".split(",");[m
[32m+[m
[32m+[m[32m    // Eleventy 1.0+: use this.inputPath and this.outputPath instead[m
[32m+[m[32m    if( inputPath.endsWith(".md") ) {[m
[32m+[m[32m      for( let word of words) {[m
[32m+[m[32m        let regexp = new RegExp("\\b(" + word + ")\\b", "gi");[m
[32m+[m[32m        if(content.match(regexp)) {[m
[32m+[m[32m          console.warn(`Spelling check (${inputPath}) Found: ${word}`);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    dir: {[m
[32m+[m[32m      includes: "/_src/layouts",[m
[32m+[m[32m      data: "/_src/data",[m
[32m+[m[32m      output: "_dist",[m
[32m+[m[32m      pathPrefix: "/Prep-Course-Stage/"[m
[32m+[m[32m    },[m
[32m+[m[32m  };[m
[32m+[m[32m};[m
