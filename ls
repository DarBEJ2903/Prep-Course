[32m+[m	[32m});[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('agregarMetodo(Persona)', function() {[m
[32m+[m	[32mit('should add the method datos to the constructor', function() {[m
[32m+[m		[32magregarMetodo();[m
[32m+[m		[32mconst persona = new Persona("Juan", "P√©rez", 22, "Saavedra 123");[m
[32m+[m		[32mexpect(persona.datos()).toBe('Juan, 22 a√±os');[m
[32m+[m	[32m});[m
[32m+[m[32m});[m
