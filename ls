[32m+[m[32m    expect(elevarAlCuadrado(7)).toBe(49);[m
[32m+[m[32m    expect(elevarAlCuadrado(0)).toBe(0);[m
[32m+[m[32m    expect(elevarAlCuadrado(-5)).toBe(25);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('elevarAlCubo(num)', function() {[m
[32m+[m[32m  it('Deberia devolver el numero elevado al cubo', function() {[m
[32m+[m[32m    expect(elevarAlCubo(3)).toBe(27);[m
[32m+[m[32m    expect(elevarAlCubo(0)).toBe(0);[m
[32m+[m[32m    expect(elevarAlCubo(-5)).toBe(-125);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('elevar(num, exponent)', function() {[m
[32m+[m[32m  it('Deberia devolver el numero elevado al exponente indicado', function() {[m
[32m+[m[32m    expect(elevar(2, 2)).toBe(4);[m
[32m+[m[32m    expect(elevar(2, 3)).toBe(8);[m
[32m+[m[32m    expect(elevar(0, 5)).toBe(0);[m
[32m+[m[32m    expect(elevar(10, 1)).toBe(10);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('redondearNumero(num)', function() {[m
[32m+[m[32m  it('Deberia devolver el numero redondeado', function() {[m
[32m+[m[32m    expect(redondearNumero(1.5)).toBe(2);[m
[32m+[m[32m    expect(redondearNumero(2)).toBe(2);[m
[32m+[m[32m    expect(redondearNumero(0.1)).toBe(0);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('redondearHaciaArriba(num)', function() {[m
[32m+[m[32m  it('Deberia devolver el numero redondeado para arriba', function() {[m
[32m+[m[32m    expect(redondearHaciaArriba(1.5)).toBe(2);[m
[32m+[m[32m    expect(redondearHaciaArriba(2)).toBe(2);[m
[32m+[m[32m    expect(redondearHaciaArriba(0.1)).toBe(1);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('numeroRandom()', function() {[m
[32m+[m[32m  it('Deber√≠a devolver un n√∫mero entre 0 y 1', function() {[m
[32m+[m[32m    expect(typeof numeroRandom()).toBe('number');[m
[32m+[m[32m    expect(numeroRandom()).toBeGreaterThanOrEqual(0);[m
[32m+[m[32m    expect(numeroRandom()).toBeLessThan(1);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('esPositivo(numero)', function() {[m
[32m+[m[32m  it('Deber√≠a devolver el string \Es positivo √≥ Es negativo\ seg√∫n corresponda', function() {[m
[32m+[m[32m    expect(esPositivo(0)).toBe(false);[m
[32m+[m[32m    expect(esPositivo(23)).toBe('Es positivo');[m
[32m+[m[32m    expect(esPositivo(-2)).toBe('Es negativo');[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('agregarSimboloExclamacion(str)', function() {[m
[32m+[m[32m  it('Deberia agregar un signo de exclamacion al final del string', function() {[m
[32m+[m[32m    expect(agregarSimboloExclamacion('hello world')).toBe('hello world!');[m
[32m+[m[32m    expect(agregarSimboloExclamacion('Soy Henry')).toBe('Soy Henry!');[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('combinarNombres(firstName, lastName)', function() {[m
[32m+[m[32m  it('Deberia devolver los strings combinados con un espacio en el medio', function() {[m
[32m+[m[32m    expect(combinarNombres('hello', 'world')).toBe('hello world');[m
[32m+[m[32m    expect(combinarNombres('Soy', 'Henry')).toBe('Soy Henry');[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('obtenerSaludo(name)', function() {[m
[32m+[m[32m  it('Deberia devolver el string \'Hola {name}!\'', function() {[m
[32m+[m[32m    expect(obtenerSaludo('Martin')).toBe('Hola Martin!');[m
[32m+[m[32m    expect(obtenerSaludo('Antonio')).toBe('Hola Antonio!');[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('obtenerAreaRectangulo(alto, ancho)', function() {[m
[32m+[m[32m  it('Deberia retornar el area correcta del rectangulo', function() {[m
[32m+[m[32m    expect(obtenerAreaRectangulo(2, 2)).toBe(4);[m
[32m+[m[32m    expect(obtenerAreaRectangulo(3, 6)).toBe(18);[m
[32m+[m[32m    expect(obtenerAreaRectangulo(0, 2)).toBe(0);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('retornarPerimetro(lado)', function() {[m
[32m+[m[32m  it('Deberia retornar el per√≠metro correcto del cuadrado', function() {[m
[32m+[m[32m    expect(retornarPerimetro(2)).toBe(8);[m
[32m+[m[32m    expect(retornarPerimetro(8)).toBe(32);[m
[32m+[m[32m    expect(retornarPerimetro(0)).toBe(0);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m
[32m+[m[32mdescribe('areaDelTriangulo( base, altura)', function() {[m
[32m+[m[32m  it('Deberia retornar el area correcta del tri√°ngulo', function() {[m
[32m+[m[32m    expect(areaDelTriangulo(10, 5)).toBe(25);[m
[32m+[m[32m    expect(areaDelTriangulo(20, 10)).toBe(100);[m
[32m+[m[32m    expect(areaDelTriangulo(0, 10)).toBe(0);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('deEuroAdolar(euro)', function() {[m
[32m+[m[32m  it('Deberia retornar el valor correcto de euro a dolar', function() {[m
[32m+[m[32m    expect(deEuroAdolar(100)).toBe(120);[m
[32m+[m[32m    expect(deEuroAdolar(200)).toBe(240);[m
[32m+[m[32m    expect(deEuroAdolar(50)).toBe(60);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('esVocal(letra)', function() {[m
[32m+[m[32m  it('Deberia devolver el string "Dato incorrecto", si el valor ingresado tiene m√°s de una letra', function() {[m
[32m+[m[32m    expect(esVocal("la")).toBe("Dato incorrecto");[m
[32m+[m[32m    expect(esVocal("pero")).toBe("Dato incorrecto");[m
[32m+[m[32m  });[m
[32m+[m[32m  it('Deberia devolver el string "Dato incorrecto", si el valor ingresado no es vocal', function() {[m
[32m+[m[32m    expect(esVocal("l")).toBe("Dato incorrecto");[m
[32m+[m[32m    expect(esVocal("p")).toBe("Dato incorrecto");[m
[32m+[m[32m  });[m
[32m+[m[32m  it('Deberia devolver el string "Es vocal", si el valor ingresado lo es', function() {[m
[32m+[m[32m    expect(esVocal('a')).toBe('Es vocal');[m
[32m+[m[32m    expect(esVocal('e')).toBe('Es vocal');[m
[32m+[m[32m    expect(esVocal('i')).toBe('Es vocal');[m
[32m+[m[32m    expect(esVocal('o')).toBe('Es vocal');[m
[32m+[m[32m    expect(esVocal('u')).toBe('Es vocal');[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
