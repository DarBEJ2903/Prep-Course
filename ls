[32m+[m
[32m+[m[32mdescribe('obtenerMayor(x, y)', function() {[m
[32m+[m[32m  it('should return x if it is larger than y', function() {[m
[32m+[m[32m    expect(obtenerMayor(10, 5)).toBe(10);[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return y if it is larger than x', function() {[m
[32m+[m[32m    expect(obtenerMayor(50, 100)).toBe(100);[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return either one if they are the same', function() {[m
[32m+[m[32m    expect(obtenerMayor(1000, 1000)).toBe(1000);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('mayoriaDeEdad(edad)', function() {[m
[32m+[m[32m  it('should return \'Allowed\'if edad is equal or greater than 18', function() {[m
[32m+[m[32m    expect(mayoriaDeEdad(18)).toBe('Allowed');[m
[32m+[m[32m    expect(mayoriaDeEdad(35)).toBe('Allowed');[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return \'Not allowed\'if edad is less than 18', function() {[m
[32m+[m[32m    expect(mayoriaDeEdad(7)).toBe('Not allowed');[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('conection(status)', function() {[m
[32m+[m[32m  it('should return the conection state of user', function() {[m
[32m+[m[32m    expect(conection(1)).toBe('Online');[m
[32m+[m[32m    expect(conection(2)).toBe('Away');[m
[32m+[m[32m    expect(conection(3)).toBe('Offline');[m
[32m+[m[32m    expect(conection()).toBe('Offline');[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('saludo(idioma)', function() {[m
[32m+[m[32m  it('should return \'Guten Tag!\' for aleman', function() {[m
[32m+[m[32m    expect(saludo('aleman')).toBe('Guten Tag!');[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return \'Hola!\' for ingles', function() {[m
[32m+[m[32m    expect(saludo('ingles')).toBe('Hello!');[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return \'Ni Hao!\' for mandarin', function() {[m
[32m+[m[32m    expect(saludo('mandarin')).toBe('Ni Hao!');[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return \'Hola!\' if no argument is passed in or if a idioma beyond ingles, mandarin, and aleman is passed in.', function() {[m
[32m+[m[32m    expect(saludo('frances')).toBe('Hola!');[m
[32m+[m[32m    expect(saludo()).toBe('Hola!');[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('colors(color)', function() {[m
[32m+[m[32m  it('should return \'This is {color}\'', function() {[m
[32m+[m[32m    expect(colors("blue")).toBe('This is blue');[m
[32m+[m[32m    expect(colors("red")).toBe('This is red');[m
[32m+[m[32m    expect(colors("green")).toBe('This is green');[m
[32m+[m[32m    expect(colors("orange")).toBe('This is orange');[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return \'Color not found\' if there is no argument or the argument is none of the expected ones', function() {[m
[32m+[m[32m    expect(colors()).toBe('Color not found');[m
[32m+[m[32m    expect(colors("pink")).toBe('Color not found');[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('esDiezOCinco(num)', function() {[m
[32m+[m[32m  it('should return true if num is 10 or 5', function() {[m
[32m+[m[32m    expect(esDiezOCinco(10)).toBe(true);[m
[32m+[m[32m    expect(esDiezOCinco(5)).toBe(true);[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return false if num is not 10 or 5', function() {[m
[32m+[m[32m    expect(esDiezOCinco(11)).toBe(false);[m
[32m+[m[32m    expect(esDiezOCinco(6)).toBe(false);[m
[32m+[m[32m    expect(esDiezOCinco(0)).toBe(false);[m
[32m+[m[32m    expect(esDiezOCinco(5.01)).toBe(false);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('estaEnRango(num)', function() {[m
[32m+[m[32m  it('should return true if num is inside range', function() {[m
[32m+[m[32m    expect(estaEnRango(35.5)).toBe(true);[m
[32m+[m[32m    expect(estaEnRango(40)).toBe(true);[m
[32m+[m[32m    expect(estaEnRango(49)).toBe(true);[m
[32m+[m[32m    expect(estaEnRango(21)).toBe(true);[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return false if outside of range', function() {[m
[32m+[m[32m    expect(estaEnRango(10)).toBe(false);[m
[32m+[m[32m    expect(estaEnRango(20)).toBe(false);[m
[32m+[m[32m    expect(estaEnRango(50)).toBe(false);[m
[32m+[m[32m    expect(estaEnRango(100)).toBe(false);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('esEntero(num)', function() {[m
[32m+[m[32m  it('should return true if num is 5', function() {[m
[32m+[m[32m    expect(esEntero(5)).toBe(true);[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return false if num is 0.5', function() {[m
[32m+[m[32m    expect(esEntero(0.5)).toBe(false);[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return true if num is -20', function() {[m
[32m+[m[32m    expect(esEntero(-20)).toBe(true);[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return true for 0', function() {[m
[32m+[m[32m    expect(esEntero(0)).toBe(true);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('fizzBuzz(num)', function() {[m
[32m+[m[32m  it('should return fizz if divisible by 3', function() {[m
[32m+[m[32m    expect(fizzBuzz(9)).toBe('fizz');[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return buzz if divisible by 5', function() {[m
[32m+[m[32m    expect(fizzBuzz(10)).toBe('buzz');[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return fizzbuzz if divisible by 3 and 5', function() {[m
[32m+[m[32m    expect(fizzBuzz(15)).toBe('fizzbuzz');[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return num if not divisible by 3 or 5', function() {[m
[32m+[m[32m    expect(fizzBuzz(4)).toBe(4);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('operadoresLogicos(num1, num2, num3)', function() {[m
[32m+[m[32m  it('should return \'N√∫mero 1 es mayor y positivo\' if num1 is positive and if it is greater than num2 and num3', function() {[m
[32m+[m[32m    expect(operadoresLogicos(50, 2, 15)).toBe('N√∫mero 1 es mayor y positivo');[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return \'Hay negativos\' if any of the arguments are negative', function() {[m
[32m+[m[32m    expect(operadoresLogicos(80, -1, 2)).toBe('Hay negativos');[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return num3 + 1 if num3 is greater than num1 and num2', function() {[m
[32m+[m[32m    expect(operadoresLogicos(1, 3, 10)).toBe(11);[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return \'Error\' if any of the arguments are equal 0', function() {[m
[32m+[m[32m    expect(operadoresLogicos(1, 0, 10)).toBe('Error');[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return false if none of the conditions are met', function() {[m
[32m+[m[32m    expect(operadoresLogicos(10, 30, 6)).toBe(false);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('esPrimo(num)', function() {[m
[32m+[m[32m  it('should return true if num is prime', function() {[m
[32m+[m[32m    expect(esPrimo(7)).toBe(true);[m
[32m+[m[32m    expect(esPrimo(97)).toBe(true);[m
[32m+[m[32m    expect(esPrimo(11)).toBe(true);[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return false if num is not prime', function() {[m
[32m+[m[32m    expect(esPrimo(9)).toBe(false);[m
[32m+[m[32m    expect(esPrimo(4)).toBe(false);[m
[32m+[m[32m    expect(esPrimo(10)).toBe(false);[m
[32m+[m[32m    expect(esPrimo(100)).toBe(false);[m
[32m+[m[32m    expect(esPrimo(0)).toBe(false);[m
[32m+[m[32m    expect(esPrimo(1)).toBe(false);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe ('esVerdadero(valor)', function() {[m
[32m+[m[32m  it('should return Soy verdadero if valor is true', function() {[m
[32m+[m[32m    expect(esVerdadero(true)).toBe("Soy verdadero");[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should return Soy falso if valor is false', function() {[m
[32m+[m[32m    expect(esVerdadero(false)).toBe("Soy falso");[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe ('tablaDelSeis()', function() {[m
[32m+[m[32m  it('should return multiplication table of 6', function() {[m
[32m+[m[32m    expect(tablaDelSeis()).toEqual([0 , 6, 12, 18, 24, 30, 36, 42, 48, 54, 60]);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe ('tieneTresDigitos(numero)', function() {[m
[32m+[m[32m  it('should return true if numero has 3 digits', function() {[m
[32m+[m[32m    expect(tieneTresDigitos(100)).toBe(true);[m
[32m+[m[32m    expect(tieneTresDigitos(820)).toBe(true);[m
[32m+[m[32m    expect(tieneTresDigitos(900)).toBe(true);[m
[32m+[m[32m    expect(tieneTresDigitos(10)).toBe(false);[m
[32m+[m[32m    expect(tieneTresDigitos(20000)).toBe(false);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('doWhile(numero)', function() {[m
[32m+[m[32m  it('should add 5 to number until reaching a limit of 8 times and return the sum value', function() {[m
[32m+[m[32m    expect(doWhile(5)).toBe(45);[m
[32m+[m[32m    expect(doWhile(100)).toBe(140);[m
[32m+[m[32m    expect(doWhile(-1)).toBe(39);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
