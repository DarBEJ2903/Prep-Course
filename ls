[32m+[m	[32mit('should add a reverse string method to the String prototype that returns a reversed version of the string', function() {[m
[32m+[m		[32magregarStringInvertida();[m
[32m+[m		[32mconst str = 'Hello';[m
[32m+[m		[32mexpect(str.reverse()).toBe('olleH');[m
[32m+[m	[32m});[m
[32m+[m[32m});[m
