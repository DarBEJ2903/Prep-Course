[32m+[m	[32mit('should return a user constructor that correctly builds user objects', function() {[m
[32m+[m		[32mconst Usuario = crearUsuario();[m
[32m+[m		[32mconst user = new Usuario({usuario: 'jssamuel', nombre: 'Samuel', email: 'samuel@email.com', password: 'LoveJS' });[m
[32m+[m		[32mexpect(user.usuario).toBe('jssamuel');[m
[32m+[m		[32mexpect(user.nombre).toBe('Samuel');[m
[32m+[m		[32mexpect(user.email).toBe('samuel@email.com');[m
[32m+[m		[32mexpect(user.password).toBe('LoveJS');[m
[32m+[m[41m    [m	[32mexpect(user.saludar()).toBe('Hola, mi nombre es Samuel');[m
[32m+[m	[32m});[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('agregarMetodoPrototype(Constructor)', function() {[m
[32m+[m	[32mit('should add the method saludar to the constructor', function() {[m
[32m+[m		[32mfunction Test() {[m
[32m+[m			[32mthis.test = true;[m
[32m+[m		[32m}[m
[32m+[m		[32magregarMetodoPrototype(Test);[m
[32m+[m		[32mconst test = new Test();[m
[32m+[m		[32mexpect(test.saludar()).toBe('Hello World!');[m
[32m+[m	[32m});[m
[32m+[m[32m});[m
[32m+[m
