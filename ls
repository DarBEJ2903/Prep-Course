[36m@@ -0,0 +1,24 @@[m
[32m+[m[32m<!DOCTYPE html>[m
[32m+[m[32m<html>[m
[32m+[m[32m<head>[m
[32m+[m[32m  <title>Estilos</title>[m
[32m+[m[32m  <link rel="stylesheet" href="./externo.css">[m
[32m+[m[32m  <style>[m
[32m+[m[32m      p {[m
[32m+[m[32m        color: blue;[m
[32m+[m[32m        font-size: 20px;[m
[32m+[m[32m      }[m
[32m+[m[32m      span {[m
[32m+[m[32m        color: green;[m
[32m+[m[32m      }[m
[32m+[m[32m  </style>[m
[32m+[m[32m</head>[m
[32m+[m[32m<body>[m
[32m+[m[32m  <p style="color:red">Hola</p>[m
[32m+[m[32m  <p class='amarillo'>Hola</p>[m
[32m+[m[32m  <p>Hola</p>[m
[32m+[m[32m  <span> Chao </span>[m
[32m+[m[32m  <p id='unico'>Hola</p>[m
[32m+[m[32m  <p class='amarillo'>Hola</p>[m
[32m+[m[32m</body>[m
[32m+[m[32m</html>[m
