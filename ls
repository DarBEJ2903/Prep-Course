[33mcommit 4a00a18456c532a1929fbedce25c6ed2bfcabd85[m
Author: Gustavo Martinez Trejo <martinez.trejo.gustavo.a@gmail.com>
Date:   Wed Jan 12 13:02:11 2022 -0300

    add reporter folder

[1mdiff --git a/.reporter/index.js b/.reporter/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..2c387b6[m
[1m--- /dev/null[m
[1m+++ b/.reporter/index.js[m
[36m@@ -0,0 +1,109 @@[m
[32m+[m[32mconst gitRemoteOriginUrl = require("git-remote-origin-url");[m
[32m+[m[32mconst GitUrlParse = require("git-url-parse");[m
[32m+[m[32mconst fs = require('fs');[m
[32m+[m[32mconst os = require('os');[m
[32m+[m[32mconst axios = require('axios');[m
[32m+[m[32mconst path = require('path');[m
[32m+[m
[32m+[m[32mconst WINDOWS = 'Windows_NT';[m
[32m+[m[32mconst MACOS = 'Darwin';[m
[32m+[m[32mconst LINUX = 'Linux';[m
[32m+[m[32mconst USERNAME = os.userInfo().username;[m
[32m+[m[32mconst WINDOWS_PATH = `C:\\Users\\${USERNAME}\\.gitconfig`;[m
[32m+[m[32mconst MACOS_PATH = `${os.homedir()}/.gitconfig`;[m
[32m+[m[32mconst LINUX_PATH = `${os.homedir()}/.gitconfig`;[m
[32m+[m[32mfunction getGitEmail() {[m
[32m+[m[32m  switch (os.type()) {[m
[32m+[m[32m    case WINDOWS:[m
[32m+[m[32m      return getEmail(WINDOWS_PATH);[m
[32m+[m[32m    case MACOS:[m
[32m+[m[32m      return getEmail(MACOS_PATH);[m
[32m+[m[32m    case LINUX:[m
[32m+[m[32m      return getEmail(LINUX_PATH);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32mfunction searchEmail(str) {[m
[32m+[m[32m  var re = /(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))/;[m
[32m+[m[32m  return re.exec(str);[m
[32m+[m[32m}[m
[32m+[m[32mfunction getEmail(PATH) {[m
[32m+[m	[32mtry {[m
[32m+[m		[32mconst email = searchEmail(fs[m
[32m+[m			[32m.readFileSync(PATH)[m
[32m+[m[32m      .toString());[m
[32m+[m		[32mif (email.length > 0) {[m
[32m+[m			[32mreturn email[0];[m
[32m+[m		[32m} else {[m
[32m+[m			[32mconsole.log(`[m
[32m+[m[32m            configura tu cuenta de git por favor![m
[32m+[m[32m            https://git-scm.com/book/es/v2/Personalizaci%C3%B3n-de-Git-Configuraci%C3%B3n-de-Git`);[m
[32m+[m			[32mprocess.exit()[m
[32m+[m		[32m}[m
[32m+[m	[32m} catch (error) {[m
[32m+[m		[32mconsole.log([m
[32m+[m			[32m`Tienes que instalar git![m
[32m+[m[32m        https://git-scm.com/book/es/v2/Inicio---Sobre-el-Control-de-Versiones-Instalaci%C3%B3n-de-Git`[m
[32m+[m		[32m);[m
[32m+[m		[32mprocess.exit()[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction readFile() {[m
[32m+[m[32m  try{[m
[32m+[m[32m    const lt = fs.readFileSync('./.reporter/lt.json');[m
[32m+[m[32m    return JSON.parse(lt);[m
[32m+[m[32m  } catch (e) {[m
[32m+[m[32m    return {};[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mmodule.exports = function report(data) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    const lastRun = readFile();[m
[32m+[m[32m    const username = getGitEmail();[m
[32m+[m[32m    gitRemoteOriginUrl()[m
[32m+[m[32m        .then((remote) => {[m
[32m+[m[32m          const { name: repo, owner: github } = GitUrlParse(remote);[m
[32m+[m[32m          const pedidos = data.testResults.map(test => {[m
[32m+[m[32m            if(!lastRun[test.testFilePath]) {[m
[32m+[m[32m              lastRun[test.testFilePath] = {[m
[32m+[m[32m                passing: test.numPassingTests,[m
[32m+[m[32m                tries: 0,[m
[32m+[m[32m              }[m
[32m+[m[32m            }[m
[32m+[m[32m            if(lastRun[test.testFilePath].passing != test.numPassingTests || lastRun[test.testFilePath].tries == 0) {[m
[32m+[m[32m              const tries = lastRun[test.testFilePath].tries;[m
[32m+[m[32m              lastRun[test.testFilePath].passing = test.numPassingTests;[m
[32m+[m[32m              lastRun[test.testFilePath].tries = 1;[m
[32m+[m[32m              return axios.post('https://proxy.soyhenry.com:3001/m0/grade', {[m
[32m+[m[32m                "pending": test.numPendingTests,[m
[32m+[m[32m                "passing": test.numPassingTests,[m
[32m+[m[32m                "failed": test.numFailingTests,[m
[32m+[m[32m                "runtime": test.perfStats.runtime,[m
[32m+[m[32m                "slow": test.perfStats.slow,[m
[32m+[m[32m                "file": path.basename(test.testFilePath),[m
[32m+[m[32m                "repo": repo,[m
[32m+[m[32m                "github": github,[m
[32m+[m[32m                "username": username,[m
[32m+[m[32m                "tries": tries[m
[32m+[m[32m              });[m
[32m+[m[32m          } else {[m
[32m+[m[32m              lastRun[test.testFilePath].tries++;[m
[32m+[m[32m              lastRun[test.testFilePath].passing = test.numPassingTests;[m
[32m+[m[32m              return Promise.resolve();[m
[32m+[m[32m          }[m
[32m+[m[32m          });[m
[32m+[m[32m          return Promise.all(pedidos);[m
[32m+[m[32m        })[m
[32m+[m[32m        .then((results) => {[m
[32m+[m[32m          fs.writeFileSync('./.reporter/lt.json', JSON.stringify(lastRun));[m
[32m+[m[32m        })[m
[32m+[m[32m        .catch((err) => {[m
[32m+[m[32m          console.log(err);[m
[32m+[m[32m        });[m
[32m+[m[32m  } catch (error) {[m
[32m+[m[32m    console.error(error);[m
[32m+[m[32m  }[m
[32m+[m[32m  return data;[m
[32m+[m[32m};[m

