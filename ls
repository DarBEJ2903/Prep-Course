[32m+[m[32mdescribe('devolverPrimerElemento(arr)', function() {[m
[32m+[m[32m  it('should return the first item in the array', function() {[m
[32m+[m[32m    expect(devolverPrimerElemento([10, 10, 16, 12])).toBe(10);[m
[32m+[m[32m    expect(devolverPrimerElemento([97, 100, 80, 55, 72, 94])).toBe(97);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('devolverUltimoElemento(arr)', function() {[m
[32m+[m[32m  it('should return the last item in the array', function() {[m
[32m+[m[32m    expect(devolverUltimoElemento([10, 10, 16, 12])).toBe(12);[m
[32m+[m[32m    expect(devolverUltimoElemento([97, 100, 80, 55, 72, 94])).toBe(94);[m
[32m+[m[32m    expect(devolverUltimoElemento(['hi', 'there', 'how', 'are', 'you', 'doing?'])).toBe('doing?');[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('obtenerLargoDelArray(arr)', function() {[m
[32m+[m[32m  it('should return the length of the array', function() {[m
[32m+[m[32m    expect(obtenerLargoDelArray([10, 10, 16, 12])).toBe(4);[m
[32m+[m[32m    expect(obtenerLargoDelArray([97, 100, 80, 55, 72, 94])).toBe(6);[m
[32m+[m[32m    expect(obtenerLargoDelArray(['hi', 'there', 'how', 'are', 'you', 'doing?'])).toBe(6);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('incrementarPorUno(arr)', function() {[m
[32m+[m[32m  it('should return the array with each value incremented by one', function() {[m
[32m+[m[32m    expect(incrementarPorUno([10, 10, 16, 12])).toEqual([11, 11, 17, 13]);[m
[32m+[m[32m    expect(incrementarPorUno([97, 100, 80, 55, 72, 94])).toEqual([98, 101, 81, 56, 73, 95]);[m
[32m+[m[32m    expect(incrementarPorUno([])).toEqual([]);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('agregarItemAlFinalDelArray(arr, item)', function() {[m
[32m+[m[32m  it('should return the array with the item added to the end', function() {[m
[32m+[m[32m    expect(agregarItemAlFinalDelArray([10, 10, 16, 12], 10)).toEqual([10, 10, 16, 12, 10]);[m
[32m+[m[32m    expect(agregarItemAlFinalDelArray([97, 100, 80, 55, 72, 94], 'Hola')).toEqual([97, 100, 80, 55, 72, 94, 'Hola']);[m
[32m+[m[32m    expect(agregarItemAlFinalDelArray([], true)).toEqual([true]);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('agregarItemAlComienzoDelArray(arr, item)', function() {[m
[32m+[m[32m  it('should return the array with the item added to the front', function() {[m
[32m+[m[32m    expect(agregarItemAlComienzoDelArray([10, 10, 16, 12], 10)).toEqual([10, 10, 10, 16, 12]);[m
[32m+[m[32m    expect(agregarItemAlComienzoDelArray([97, 100, 80, 55, 72, 94], 'Hola')).toEqual(['Hola', 97, 100, 80, 55, 72, 94]);[m
[32m+[m[32m    expect(agregarItemAlComienzoDelArray([], true)).toEqual([true]);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('dePalabrasAFrase(words)', function() {[m
[32m+[m[32m  it('should return a string that has all of the words from the array separated by spaces', function() {[m
[32m+[m[32m    expect(dePalabrasAFrase(['Henry', 'JavaScript', 'Class'])).toBe('Henry JavaScript Class');[m
[32m+[m[32m    expect(dePalabrasAFrase(['Henry'])).toBe('Henry');[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('arrayContiene(arr, item)', function() {[m
[32m+[m[32m  it('should return true if the array contains the item', function() {[m
[32m+[m[32m    expect(arrayContiene([10, 10, 16, 12], 10)).toBe(true);[m
[32m+[m[32m    expect(arrayContiene([97, 100, 80, 55, 72, 94], 'Hola')).toBe(false);[m
[32m+[m[32m    expect(arrayContiene([], true)).toBe(false);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('agregarNumeros(numbers)', function() {[m
[32m+[m[32m  it('should add all of the numbers in the array together and return the sum', function() {[m
[32m+[m[32m    expect(agregarNumeros([10, 10, 16])).toBe(36);[m
[32m+[m[32m    expect(agregarNumeros([97, 100])).toBe(197);[m
[32m+[m[32m    expect(agregarNumeros([0])).toBe(0);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('promedioResultadosTest(testScores)', function() {[m
[32m+[m[32m  it('should return the average test score', function() {[m
[32m+[m[32m    expect(promedioResultadosTest([10, 10, 16, 12])).toBe(12);[m
[32m+[m[32m    expect(promedioResultadosTest([97, 100, 80, 55, 72, 94])).toBe(83);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('numeroMasGrande(numbers)', function() {[m
[32m+[m[32m  it('should return the largest number', function() {[m
[32m+[m[32m    expect(numeroMasGrande([10, 10, 16, 12])).toBe(16);[m
[32m+[m[32m    expect(numeroMasGrande([97, 100, 80, 55, 72, 94])).toBe(100);[m
[32m+[m[32m    expect(numeroMasGrande([97, 10000, 80, 55, 7.2, -94])).toBe(10000);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('multiplicarArgumentos()', function () {[m
[32m+[m[32m  it('should return the product of all the arguments', function () {[m
[32m+[m[32m    const product = multiplicarArgumentos(5, 5);[m
[32m+[m[32m    const product2 = multiplicarArgumentos();[m
[32m+[m[32m    const product3 = multiplicarArgumentos(3, 3, 3, 3);[m
[32m+[m[32m    const product4 = multiplicarArgumentos(1);[m
[32m+[m[32m    const product5 = multiplicarArgumentos(10, 0, 10);[m
[32m+[m[32m    expect(product).toBe(25);[m
[32m+[m[32m    expect(product2).toBe(0);[m
[32m+[m[32m    expect(product3).toBe(81);[m
[32m+[m[32m    expect(product4).toBe(1);[m
[32m+[m[32m    expect(product5).toBe(0);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('cuentoElementos(arreglo)', function() {[m
[32m+[m[32m  it('should return a counter with the total of numbers greater than 18', function() {[m
[32m+[m[32m    expect(cuentoElementos([10, 10, 18, 21])).toBe(1);[m
[32m+[m[32m    expect(cuentoElementos([97, 100, 190, 5, 7, 9])).toBe(3);[m
[32m+[m[32m    expect(cuentoElementos([97, 20, 90, 50, 7, 709])).toBe(5);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('diaDeLaSemana(numeroDeDia)', function() {[m
[32m+[m[32m  it('should return he name of the day associated with that number', function() {[m
[32m+[m[32m    expect(diaDeLaSemana(3)).toBe("Es dia Laboral");[m
[32m+[m[32m    expect(diaDeLaSemana(2)).toBe("Es dia Laboral");[m
[32m+[m[32m    expect(diaDeLaSemana(1)).toBe("Es fin de semana");[m
[32m+[m[32m    expect(diaDeLaSemana(7)).toBe("Es fin de semana");[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('empiezaConNueve(n))', function() {[m
[32m+[m[32m  it('should return true or false in case the received number starts with nine', function() {[m
[32m+[m[32m    expect(empiezaConNueve(98)).toBe(true);[m
[32m+[m[32m    expect(empiezaConNueve(9)).toBe(true);[m
[32m+[m[32m    expect(empiezaConNueve(2)).toBe(false);[m
[32m+[m[32m    expect(empiezaConNueve(7)).toBe(false);[m
[32m+[m[32m    expect(empiezaConNueve(-5)).toBe(false);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('todosIguales(arreglo)', function() {[m
[32m+[m[32m  it('should return true if all the elements of the array are equal and false otherwise', function() {[m
[32m+[m[32m    expect(todosIguales([20, 20, 20, 20 ])).toBe(true);[m
[32m+[m[32m    expect(todosIguales([97, 100, 190, 9])).toBe(false);[m
[32m+[m[41m   [m
[32m+[m[32m    expect(todosIguales([230, 230, 230, 230 ])).toBe(true);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('mesesDelA√±o(array)', function() {[m
[32m+[m[32m  it('should return a new array with the requested months', function() {[m
[32m+[m[32m    var ok = ["Marzo", "Diciembre", "Abril", "Junio", "Julio", "Noviembre", "Enero", "Mayo", "Febrero"];[m
[32m+[m[32m    var wrong = ["Marzo", "Diciembre", "Julio", "Noviembre"];[m
[32m+[m[32m    expect(mesesDelA√±o(ok)).toEqual(["Marzo", "Noviembre", "Enero"]);[m
[32m+[m[32m    expect(mesesDelA√±o(wrong)).toEqual("No se encontraron los meses pedidos");[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('mayorACien(array)', function() {[m
[32m+[m[32m  it('should return an array with values grater than 100', function() {[m
[32m+[m[32m    var integers = [100, 4, 56, 78, 200, 120, 7, 160, 148, 22];[m
[32m+[m[32m    expect(mayorACien(integers)).toEqual([200, 120, 160, 148]);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('breakStatement(numero)', function() {[m
[32m+[m[32m  it('should return an array with the values ‚Äã‚Äãresulting from adding 2 in each iteration', function() {[m
[32m+[m[32m    expect(breakStatement(50)).toEqual([52, 54, 56, 58, 60, 62, 64, 66, 68, 70]);[m
[32m+[m[32m  });[m
[32m+[m[32m  it('should stop the execution if the sum value and the iteration number match', function() {[m
[32m+[m[32m    expect(breakStatement(-4)).toEqual('Se interrumpi√≥ la ejecuci√≥n');[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('continueStatement(numero)', function() {[m
[32m+[m[32m  it('should return an array with the values ‚Äã‚Äãresulting from adding 2 in each iteration when the iteration number is different from 5', function() {[m
[32m+[m[32m    expect(continueStatement(50)).toEqual([52, 54, 56, 58, 60, 62, 64, 66, 68]);[m
[32m+[m[32m    expect(continueStatement(-4)).toEqual([-2, 0, 2, 4, 6, 8, 10, 12, 14]);[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
\ No newline at end of file[m
