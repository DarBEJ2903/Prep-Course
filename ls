[32m+[m[32m// Do not change any of the function names[m
[32m+[m
[32m+[m[32mfunction mayuscula(nombre) {[m
[32m+[m[32m  //La funci√≥n recibe un nombre y debe devolver el mismo que recibe pero con su primer letra en may√∫scula[m
[32m+[m[32m  //ej: Recibe "mario" ----> Devuelve "Mario"[m
[32m+[m[32m  //Tu c√≥digo:[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction invocarCallback(cb) {[m
[32m+[m[32m  // Invoca al callback `cb`[m
[32m+[m[32m  //Tu c√≥digo:[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction operacionMatematica(n1, n2, cb) {[m
[32m+[m[32m  //Vamos a recibir una funci√≥n que realiza una operaci√≥n matem√°tica como callback junto con dos n√∫meros.[m
[32m+[m[32m  //Devolver el callback pas√°ndole como argumentos los n√∫meros recibidos.[m
[32m+[m[32m  //Tu c√≥digo:[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction sumarArray(numeros, cb) {[m
[32m+[m[32m  // Suma todos los n√∫meros enteros (int/integers) de un array ("numeros")[m
[32m+[m[32m  // Pasa el resultado a `cb`[m
[32m+[m[32m  // No es necesario devolver nada[m
[32m+[m[32m  //Tu c√≥digo:[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction forEach(array, cb) {[m
[32m+[m[32m  // Itera sobre la matriz "array" y pasa los valores al callback uno por uno[m
[32m+[m[32m  // Pista: Estar√°s invocando a `cb` varias veces (una por cada valor en la matriz)[m
[32m+[m[32m  //Tu c√≥digo:[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction map(array, cb) {[m
[32m+[m[32m  // Crea un nuevo array[m
[32m+[m[32m  // Itera sobre cada valor en "array", p√°salo a `cb` y luego ubicar el valor devuelto por `cb` en un nuevo array[m
[32m+[m[32m  // El nuevo array debe tener la misma longitud que el array del argumento[m
[32m+[m[32m  //Tu c√≥digo:[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction filter(array) {[m
[32m+[m[32m  //Filtrar todos los elementos del array que comiencen con la letra "a".[m
[32m+[m[32m  //Devolver un nuevo array con los elementos que cumplen la condici√≥n[m
[32m+[m[32m  //Tu c√≥digo:[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// No modificar nada debajo de esta l√≠nea[m
[32m+[m[32m// --------------------------------[m
[32m+[m
[32m+[m[32mmodule.exports = {[m
[32m+[m[32m  mayuscula,[m
[32m+[m[32m  invocarCallback,[m
[32m+[m[32m  operacionMatematica,[m
[32m+[m[32m  sumarArray,[m
[32m+[m[32m  forEach,[m
[32m+[m[32m  map,[m
[32m+[m[32m  filter[m
[32m+[m[32m};[m
