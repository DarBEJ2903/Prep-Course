[32m+[m[32m  it('should invoke the callback that is passed in', function() {[m
[32m+[m		[32mconst cb = jest.fn();[m
[32m+[m		[32minvocarCallback(cb);[m
[32m+[m[41m    [m	[32mexpect(cb).toHaveBeenCalled();[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('operacionMatematica(n1, n2, cb)', function() {[m
[32m+[m	[32mit('should return the callback function passing it the received arguments', function() {[m
[32m+[m		[32mconst cb = jest.fn();[m
[32m+[m		[32moperacionMatematica(100, 20, cb);[m
[32m+[m		[32mexpect(cb).toHaveBeenCalled();[m
[32m+[m	[32m});[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('sumarArray(cb)', function() {[m
[32m+[m	[32mit('should pass the sum of all array numbers to cb', function(done) {[m
[32m+[m		[32msumarArray([1, 2, 3, 4, 5], function(sum) {[m
[32m+[m			[32mexpect(sum).toBe(15);[m
[32m+[m			[32mdone();[m
[32m+[m		[32m});[m
[32m+[m	[32m});[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('forEach(arr, cb)', function() {[m
[32m+[m	[32mit('should pass all array items one by one to cb', function() {[m
[32m+[m		[32mconst nums = [];[m
[32m+[m		[32mforEach([1, 2, 3, 4, 5], function(num) {[m
[32m+[m			[32mnums.push(num);[m
[32m+[m		[32m});[m
[32m+[m		[32mexpect(nums).toEqual([1, 2, 3, 4, 5]);[m
[32m+[m	[32m});[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('map(arr, cb)', function() {[m
[32m+[m	[32mit('should return an array of all the processed array elements', function() {[m
[32m+[m		[32mconst squares = map([1, 2, 3, 4, 5], function(num) {[m
[32m+[m			[32mreturn num * num;[m
[32m+[m		[32m});[m
[32m+[m		[32mexpect(squares).toEqual([1, 4, 9, 16, 25]);[m
[32m+[m	[32m});[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mdescribe('filter(array)', function() {[m
[32m+[m	[32mit('should return an array conteining the words that starts with "a"', function() {[m
[32m+[m		[32mvar array = ['abajo', 'pera', 'escalera', 'alerta', 'indice', 'azteca', 'arbol', 'buzo'];[m
[32m+[m		[32mexpect(filter(array)).toEqual(["abajo", "alerta", "azteca", "arbol"]);[m
[32m+[m	[32m});[m
[32m+[m[32m});[m
