[32m+[m[32mconst {[m
[32m+[m[32m    crearUsuario,[m
[32m+[m	[32magregarMetodoPrototype,[m
[32m+[m	[32magregarStringInvertida,[m
[32m+[m	[32mcrearInstanciaPersona,[m
[32m+[m	[32magregarMetodo,[m
[32m+[m	[32mPersona[m
[32m+[m[32m} = require('../homework');[m
